Keep make from complaining "Makefile:62: target `.' given more than once in the
same rule.".

--- cctools-855/as/Makefile.orig	2015-02-20 21:02:15.000000000 +0100
+++ cctools-855/as/Makefile	2015-02-20 21:03:08.000000000 +0100
@@ -436,5 +436,9 @@
 		$(DSTROOT)$(LOCLIBDIR)/i860/as
 endif
 
-$(OFILE_DIRS) $(SYM_DIRS):
+$(OFILE_DIRS):
 	$(MKDIRS) $@
+ifneq "$(OBJROOT)" "$(SYMROOT)"
+$(SYM_DIRS):
+	$(MKDIRS) $@
+endif
--- cctools-855/ar/Makefile.orig	2015-02-20 21:03:37.000000000 +0100
+++ cctools-855/ar/Makefile	2015-02-20 21:04:22.000000000 +0100
@@ -59,5 +59,9 @@
 	gnutar cf - $(INSTALL_FILES) | (cd $(SRCROOT); gnutar xf -)
 	chmod 444 $(SRCROOT)/*
 
-$(OFILE_DIR) $(SYMROOT) $(DSTDIRS):
+$(OFILE_DIR) $(DSTDIRS):
 	$(MKDIRS) $@
+ifneq "$(OFILE_DIR)" "$(SYMROOT)"
+$(SYMROOT):
+	$(MKDIRS) $@
+endif
